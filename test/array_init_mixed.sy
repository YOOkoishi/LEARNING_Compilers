// 测试用例10: 混合初始化 - 更复杂的情况
// int arr[2][3][2] = {{1,2,3,4}, {5}}
// dims = [2, 3, 2], 总大小 = 12
// 
// {1,2,3,4} 在位置0，对齐到第0维，子维度[3,2]，容量6
//   - 1 填充到位置 0
//   - 2 填充到位置 1  
//   - 3 填充到位置 2
//   - 4 填充到位置 3
//   - 自动填0到位置 5 (容量6)
// {5} 在位置6，对齐到第0维，子维度[3,2]，容量6
//   - 5 填充到位置 6
//   - 自动填0到位置 11 (容量6)
// 预期: [1,2,3,4,0,0, 5,0,0,0,0,0]

int main() {
    int arr[2][3][2] = {{1,2,3,4}, {5}};
    
    // 验证每个元素
    int i = 0;
    while (i < 12) {
        putint(arr[i]);
        putch(32);
        i = i + 1;
    }
    putch(10);
    
    return 0;
}
