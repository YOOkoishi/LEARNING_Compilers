// 测试用例2: 二维数组 - 完整初始化
// int arr[2][3] = {{1, 2, 3}, {4, 5, 6}}
// 预期内存布局: [1, 2, 3, 4, 5, 6]

int main() {
    int arr[2][3] = {{1, 2, 3}, {4, 5, 6}};
    
    // 验证数组内容 (按行优先顺序访问)
    int i = 0;
    while (i < 2) {
        int j = 0;
        while (j < 3) {
            putint(arr[i * 3 + j]);  // 简化的线性访问
            putch(32); // 空格
            j = j + 1;
        }
        putch(10); // 换行
        i = i + 1;
    }
    
    return 0;
}
